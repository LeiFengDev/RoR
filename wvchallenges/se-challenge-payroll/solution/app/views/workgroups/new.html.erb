<h1>New work group</h1>

<%= form_for @workgroup, url: workgroups_path do |f| %>
  <p>
    <%= f.label :category %><br>
    <%= f.text_field :category %>
  </p>
 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>